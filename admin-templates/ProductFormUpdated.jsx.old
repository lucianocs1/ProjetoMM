                disabled={uploadingImages}
              />
            </Button>
          </Box>
        </Box>

        <Divider sx={{ my: 4 }} />

        {/* Se√ß√£o 4: Promo√ß√µes */}
        <Box sx={{ mb: 4 }}>
          <Typography variant="h6" sx={{ mb: 3, color: 'primary.main', fontWeight: 'bold' }}>
            üéØ Informa√ß√µes de Promo√ß√£o
          </Typography>
          
          <TextField
            fullWidth
            multiline
            rows={3}
            label="Detalhes da Promo√ß√£o"
            value={formData.promocoes}
            onChange={(e) => handleChange('promocoes', e.target.value)}
            helperText="Descreva ofertas especiais, descontos ou condi√ß√µes promocionais"
            variant="outlined"
            sx={{
              '& .MuiOutlinedInput-root': {
                borderRadius: 2,
              }
            }}
          />
        </Box>

        <Divider sx={{ my: 4 }} />

        {/* Bot√µes de A√ß√£o */}
        <Box sx={{ 
          display: 'flex', 
          gap: 3, 
          justifyContent: 'center',
          pt: 2
        }}>
          <Button
            type="submit"
            variant="contained"
            color="primary"
            size="large"
            startIcon={<Save />}
            disabled={uploadingImages}
            sx={{ 
              px: 4, 
              py: 1.5,
              borderRadius: 2,
              minWidth: 150
            }}
          >
            {product ? 'Atualizar' : 'Salvar'} Produto
          </Button>
          
          <Button
            type="button"
            variant="outlined"
            color="secondary"
            size="large"
            startIcon={<Cancel />}
            onClick={onCancel}
            disabled={uploadingImages}
            sx={{ 
              px: 4, 
              py: 1.5,
              borderRadius: 2,
              minWidth: 150
            }}
          >
            Cancelar
          </Button>
        </Box>
      </form>
    </Paper>
  )
}

export default ProductForm

                <Grid container spacing={2}>
                  <Grid item xs={12} sm={6}>
                    <FormControlLabel
                      control={
                        <Checkbox
                          checked={formData.isFeatured}
                          onChange={(e) => handleChange('isFeatured', e.target.checked)}
                          color="primary"

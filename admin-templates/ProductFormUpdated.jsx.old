      if (imageUrl.startsWith('/uploads') || imageUrl.startsWith('http://localhost:5006/uploads')) {
        const token = getAuthToken()
        if (token) {
          const imagePath = imageUrl.replace('http://localhost:5006', '')
          
          await fetch(`${API_BASE_URL}/images/delete`, {
            method: 'DELETE',
            headers: {
              'Authorization': `Bearer ${token}`,
              'Content-Type': 'application/json'
            },
            body: JSON.stringify({ imagePath })
          })
        }
      }
    } catch (error) {
      console.warn('Erro ao deletar imagem do servidor:', error)
    }
    
    // Remove da lista local
    const newImages = formData.imagens.filter((_, i) => i !== index)
    handleChange('imagens', newImages)
  }

